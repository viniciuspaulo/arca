{% extends 'base.html.twig' %}

{% block body %}
<div class="wrapper ">
    {% include 'componentes/sidebar.html.twig'%}
    <div class="main-panel">
        <div class="panel-header panel-header-sm">
        </div>
        <div class="content">
            <div class="row">
                <div class="col-md-12">
                    <div class="card">
                        <div class="card-header">
                            <h5 class="title">{% block cadcabecalho %}{% endblock %}</h5>
                        </div>
                        <div class="card-body">
                                {% block cadcorpo %}{% endblock %}
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
{% endblock %}